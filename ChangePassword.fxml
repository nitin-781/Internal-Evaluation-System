      <Label id="labelOldPassword" fx:id="labelOldPassword" layoutX="56.0" layoutY="90.0" text="Old Password" />
      <Label id="labelNewPassword" fx:id="labelNewPassword" layoutX="56.0" layoutY="152.0" text="New Password" />
      <CustomPasswordField id="oldPasswordField" fx:id="oldPasswordField" layoutX="196.0" layoutY="85.0" prefHeight="31.0" prefWidth="365.0" onAction="#onbtnOK" promptText="Old Password" />
      <CustomPasswordField id="newPasswordField" fx:id="newPasswordField" layoutX="196.0" layoutY="147.0" prefHeight="31.0" prefWidth="365.0" onAction="#onbtnOK" onKeyPressed="#onNewPassKeyPressed" promptText="New Password" />
      <Button id="btnOK" fx:id="btnOK" layoutX="514.0" layoutY="235.0" mnemonicParsing="false" onAction="#onbtnOK" prefHeight="40.0" prefWidth="47.0" text="OK" />
   </children>
